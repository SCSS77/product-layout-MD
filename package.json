{
    "name": "product-layout",
    "version": "1.0.0",
    "scripts": {
      "build-css": "sass css/styles.scss css/styles.css --style=compressed && postcss css/styles.css --output css/styles.min.css",
      "watch-css": "sass --watch css/styles.scss:css/styles.css --style=compressed && postcss css/styles.css --output css/styles.min.css"
    },
    "devDependencies": {
      "cssnano": "^5.0.0",
      "postcss": "^8.0.0",
      "postcss-cli": "^10.0.0",
      "sass": "^1.0.0"
    },
    "dependencies": {
      "holygrail2": "^1.2.45"
    }
  }  